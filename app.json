{
  "expo": {
    "owner": "funesgv",
    "name": "Ledfit",
    "slug": "Ledfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ledfit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ledfit-front",
      "associatedDomains": ["applinks:emerging-deer-62.clerk.accounts.dev"]
    },
    "android": {
      "package": "com.anonymous.ledfit_front",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "emerging-deer-62.clerk.accounts.dev",
              "pathPrefix": "/"
            },
            {
              "scheme": "ledfit"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "projectId": "3e1cdb34-bace-4ca7-ab4",
        "origin": false
      },
      "eas": {
        "projectId": "3e1cdb34-bace-4ca7-ab45-a467abecb563"
      },
      "clerkOAuthRedirectUrl": "ledfit://oauth-callback"
    }
  }
}
